#!/usr/bin/env php
<?php

declare(strict_types=1);

use App\Providers\AppServiceProvider;
use App\Providers\ConsoleServiceProvider;
use Dotenv\Dotenv;
use Maduser\Argon\Container\ArgonContainer;
use Maduser\Argon\Logging\LoggerServiceProvider;
use Maduser\Argon\Prophecy\Argon;
use Maduser\Argon\Prophecy\Provider\ArgonCliFoundation;

require __DIR__ . '/../../../../../vendor/autoload.php';

Dotenv::createImmutable(__DIR__ . '/../')->load();

Argon::prophecy(function (ArgonContainer $container): void {
    $container->register(ArgonCliFoundation::class);
    $container->register(ConsoleServiceProvider::class);
}, 'false');